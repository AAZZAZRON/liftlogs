{
  "expo": {
    "name": "LiftLog",
    "slug": "liftlog",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/icon.png",
    "scheme": "myapp",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "splash": {
      "image": "./assets/images/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.azron.liftlog"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      },
      "package": "com.azron.liftlog"
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": [
      "expo-router",
      "expo-font"
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "eas": {
        "projectId": "a322441f-acb6-40bf-a768-be698f175f7c"
      }
    },
    "owner": "azron",
    "updates": {
      "url": "https://u.expo.dev/a322441f-acb6-40bf-a768-be698f175f7c"
    },
    "runtimeVersion": {
      "policy": "appVersion"
    }
  }
}
